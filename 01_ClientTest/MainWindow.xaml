        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_01_ClientTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <StackPanel Orientation="Horizontal">
                <TextBox x:Name="tbMsg" Height="40" Width="270"/>
                <Button IsDefault="True" Width="65" Click="Button_Click">Send</Button>
            </StackPanel>
            <StackPanel Margin="10" Background="LightGray" MinHeight="349">
                <Label Name="lbResponce">

                </Label>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
